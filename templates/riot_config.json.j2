{
    "default_server_config": {
        "m.homeserver": {
            "base_url": "https://{{ riot_hs_url }}",
            "server_name": "Geekbundle.org"
        },
        "m.identity_server": {
            "base_url": "https://{{ riot_is_url }}"
        }
    },
    "disable_identity_server": false,
    "disable_custom_urls": false,
    "disable_guests": true,
    "disable_login_language_selector": false,
    "disable_3pid_login": false,
    "brand": "Riot",
{% if riot_integration_ui is defined and not riot_integration_ui is none %}
    "integrations_ui_url": "{{ riot_integration_ui }}",
{% endif %}
{% if riot_integration_api is defined and not riot_integration_api is none %}
    "integrations_rest_url": "{{ riot_integration_api }}",
{% endif %}
    "integrations_jitsi_widget_url": "https://scalar.vector.im/api/widgets/jitsi.html",
    "bug_report_endpoint_url": "https://riot.im/bugreports/submit",
    "defaultCountryCode": "DE",
    "showLabsSettings": true,
    "default_federate": true,
    "default_theme": "{{ riot_theme }}",
    "roomDirectory": {
        "servers": [
            "{{ hostname }}",
            "matrix.org"
        ]
    },
    "welcomeUserId": "@riot-bot:matrix.org",
    "piwik": {
        "url": "https://piwik.riot.im/",
        "whitelistedHSUrls": ["https://matrix.org"],
        "whitelistedISUrls": ["https://vector.im", "https://matrix.org"],
        "siteId": 1
    },
    "enable_presence_by_hs_url": {
        "https://matrix.org": false
    }
}
